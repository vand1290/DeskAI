{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist-examples",
    "rootDir": "./examples"
  },
  "include": ["examples/**/*"]
}
